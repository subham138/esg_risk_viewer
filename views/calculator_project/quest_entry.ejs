    function addModTabRw(id){
        var tot_row = $(`#${id} tbody tr`).length
        var emi_val = $(`#emi_fact_val_${tot_row}`).val(),
        prevMonth = $(`#repo_mode_label_${tot_row}`).val(),
        startMonth = $(`#repo_mode_label_1`).val();
        var months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
        var sel_index = months.findIndex(dt => dt == prevMonth);
        var next_mon = months[sel_index + 1 >= 12 ? 0 : sel_index + 1]
        // console.log(sel_index+1, next_mon, prevMonth, startMonth);
        
        if(next_mon != startMonth){
            $(`#${id} tbody`).append(`
                <tr>
                    <td>
                        ${next_mon}
                        <input type="hidden" id="repo_mode_label_${parseInt(tot_row) + 1}" name="repo_mode_label" value="${next_mon}">
                    </td>
                    <td>
                        <input class="form-control" id="cal_val_${parseInt(tot_row) + 1}" name="cal_val" type="text" value="" required="" onchange="calTabValue(this, 'Md')">
                    </td>
                    <td>
                        <input class="form-control" id="emi_fact_val_${parseInt(tot_row) + 1}" name="emi_fact_val" type="text" value="${emi_val}" required="" onchange="calTabValue(this, 'Md')" readonly>
                    </td>
                    <td>
                        <input class="form-control" id="co_val_${parseInt(tot_row) + 1}" name="co_val" type="text" value="" required="" onchange="calTabValue(this, 'Md')">
                    </td>
                    <td>
                        <button class="btn btn-custom" type="button" onclick="delModTabRw(this)"><i class="icofont icofont-trash"></i></button>
                    </td>
                </tr>
            `)
        }else{
            initiate_sweet_alert('', 'Dublicate month found!', 'danger')
        }
    }
    async function delModTabRw(e){
        // console.log($(e));
        var text = 'Are you sure, you want to delete?',
        title = "You won't be able to revert this!";
        if(await inititate_sweet_conf_alert(text, title) > 0){
            $(e).parent().parent().detach()
        }
    }
    
</script>